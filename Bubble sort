
class BubbleSort{
    public static void main(String args[]){
        int arr[]={22,1,34,109,4,2,0,56};
        
     for(int i=0;i<8;i++){
             for(int j=i+1;j<8;j++){
                 if(arr[i]>arr[j]){
                     int temp=arr[i];
                     arr[i]=arr[j];
                     arr[j]=temp;
                 }
             }
         }
         for(int i=0;i<8;i++){
             System.out.println(arr[i]);
         }
    }
   
}
